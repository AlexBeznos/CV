{{#each projects}}
  <div class="item mb-3">
    <div class="item-heading row align-items-center mb-2">
      <h4 class="item-title col-12 col-md-6 col-lg-8 mb-2 mb-md-0">{{name}}</h4>
      <div class="item-meta col-12 col-md-6 col-lg-4 text-muted text-left text-md-right"><a href="{{link}}" target="_blank">{{link_name}}</a></div>
      
    </div>
    <div class="item-content">
      <p>{{description}}</p>
      
      
    </div>
  </div><!--//item-->
{{/each}}
