<div class="resume-header">
  <div class="row align-items-center">
    <div class="resume-title col-12 col-md-6 col-lg-8 col-xl-9 col-print-9">
      <h2 class="resume-name mb-0 text-uppercase">{{name}}</h2>
      <div class="resume-tagline mb-3 mb-md-0">{{title}}</div>
    </div><!--//resume-title-->
    <div class="resume-contact col-12 col-md-6 col-lg-4 col-xl-3 col-print-3">
      <ul class="list-unstyled mb-0">
        <li class="mb-2"><i class="fas fa-phone-square fa-fw fa-lg mr-2 "></i><a class="resume-link" href="tel:{{contacts.phone}}">{{contacts.phone}}</a></li>
        <li class="mb-2"><i class="fas fa-envelope-square fa-fw fa-lg mr-2"></i><a class="resume-link" href="mailto:{{contacts.email}}">{{contacts.email}}</a></li>
      </ul>
    </div><!--//resume-contact-->
  </div><!--//row-->
  
</div><!--//resume-header-->
